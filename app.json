{
  "expo": {
    "name": "sistem monitoring dan pendeteksi kebakaran",
    "slug": "sistem-monitoring-dan-pendeteksi-kebakaran",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF"
      },
      "package": "com.mendigitalkan.k6embeded"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "13722b24-7d2a-4d10-ab43-04173d6770c0"
      }
    }
  },
  "plugins": [
    [
      "expo-barcode-scanner",
      {
        "cameraPermission": "Allow to access camera."
      }
    ]
  ]
}
